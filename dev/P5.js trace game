function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  
  x = mouseX;
  y = mouseY;
  
  c1 = ("Real");
  c2 = ("Real");
  c3 = ("Real");
  c4 = ("Real");
  c5 = ("Real");
  
  i1 = ("");
  i2 = ("");
  i3 = ("");
  i4 = ("");
  i5 = ("");
  i6 = ("");
  i7 = ("");
  i8 = ("");
  i9 = ("");
  i10 = ("");
  
  fill(20,140,140);
  rect(80,70,280,10);
  
  fill(20,140,140);
  rect(80,130,230,10);
  
  fill(20,130)
  rect(80,80,230,50);
  
  
  
  fill(20,140,140);
  rect(360,70,10,180);
  
  fill(20,140,140);
  rect(300,130,10,60);
  
  fill(20,130)
  rect(310,80,50,160);
  
  
   
  fill(20,140,140);
  rect(140,240,220,10);
  
  fill(20,140,140);
  rect(80,180,220,10);
  
  fill(20,130)
